@{
    Layout = null;
}
<link href="/homedoctor/css/homepage/homepage.css" rel="stylesheet" asp-append-version="true" />
<div class="body_wrapper">
    <partial name="HomeDoctor/FrontEnd/_Header" />
    <partial name="HomeDoctor/FrontEnd/_Banner" />
    <partial name="HomeDoctor/FrontEnd/_Body_HomePage" />
    <partial name="HomeDoctor/FrontEnd/_Footer" />
</div>
<script>
    if (@ViewBag.IsActivedAccount == 1) {
        Swal.fire("Xác thực tài khoản", "Xác thực tài khoản thành công. <br> Vui lòng đăng nhập để sử dụng hệ thống.", "success").then(function () {
            location.href = "/sign-in";
        });
    }
</script>
<script src="/homedoctor/js/page/homepage/homepage.js" asp-append-version="true"></script>
