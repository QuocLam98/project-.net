    .select {
        appearance: none;
        width: 100%;
        height: 44px;
        border: 1px solid rgba(0, 0, 0, 0.05);
        font-size: 14px;
        font-weight: 400;
        color: var(--Place, #333);
        background-color: white;
        padding: 7px 0px 7px 17px;
        border-radius: 10px;
