<div class="modal fade" id="booking" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="booking-modal modal-content">
            <div class="booking-modal-header">
                <div class="d-flex justify-content-between">
                    <div class="list-ratting-title">
                        Thông tin đặt lịch
                        <div class="line-mini"></div>

                    </div>
                    <div class="close" data-dismiss="modal">
                        <img src="/homedoctor/img/detail-doctor/close.svg"/>
                    </div>
                </div>
                <div style="margin-top: 10px">
                    <p class="label-bookingform required">Loại đặt lịch</p>
                    <div style="display: flex; gap: 10px; margin-top: 5px;">
                        <div style="width: 100%">
                            <input checked id="349" type="radio" value="1000001" name="question1">
                            <label style="margin-bottom: 0" for="349">Khám tại nhà</label>
                        </div>
                        <div style="width: 100%">
                            <input id="350" type="radio" value="1000002" name="question1">
                            <label style="margin-bottom: 0" for="350">Khám tại phòng khám</label>
                        </div>
                    </div>
                </div>
                <div style="display: flex; gap: 14px">
                    <div style="width: 70%">
                        <p class="label-bookingform">Tên bác sĩ</p>
                        <input class="booking-input" id="doctor-name" disabled readonly/>
                    </div>
                    <div style="width: 100%">
                        <p class="label-bookingform">Tên dịch vụ</p>
                        <input class="booking-input" id="service" disabled readonly/>
                        @* <span class="service-fee-value"><span class="column-price">1.000.000</span>đ</span> *@
                    </div>
                </div>
                <div>
                    <p class="label-bookingform required">Địa chỉ khám (Nếu khám tại nhà hãy điền địa chỉ nhà của bạn)</p>
                    <input type="text" class="booking-input" id="booking-address"/>
                </div>
                <div style="margin-top: 10px">
                    <div style="display: flex; gap: 10px; margin-top: 5px;">
                        <div style="width: 100%">
                            <input id="351" type="checkbox" value="1000001" name="question2">
                            <label style="margin-bottom: 0" for="351">Thêm người phụ thuộc</label>
                        </div>
                    </div>
                </div>
                <div style="display: flex; gap: 14px;">
                    <div style="width: 100%">
                        <p class="label-bookingform required">Họ tên người khám</p>
                        <input class="booking-input" id="name"/>
                    </div>
                    <div style="width: 100%">
                        <p class="label-bookingform required">Số điện thoại</p>
                        <input class="booking-input" id="phone-number"/>
                    </div>
                </div>
                <div style="display: flex; gap: 14px;">
                    <div style="width: 100%">
                        <p class="label-bookingform required">Ngày đặt</p>
                        <input class="booking-input date datepicker2" id="day" placeholder="Chọn ngày"/>
                    </div>
                    <div style="width: 100%">
                        <p class="label-bookingform required">Giờ đặt</p>
                        <select class="booking-input " data-control="select2" id="time">


                        </select>
                    </div>
                </div>

                <div>
                    <p class="label-bookingform">Ghi chú</p>
                    <textarea class="booking-input" id="description"></textarea>
                </div>
                <div class="form-check d-flex check-confirm-term-of-service">
                    <input type="checkbox" class="form-check-input" value="" id="pxp-confirmCheckbox"/>
                    <label class="form-check-label" for="pxp-confirmCheckbox">
                        Tôi đồng ý với <a href="/chinh-sach-bao-mat">Điều khoản dịch vụ</a> và <a href="/chinh-sach-bao-mat">Chính sách bảo mật</a>
                    </label>
                </div>
            </div>
            <div class="booking-footer">
                <a class="btn-booking" onclick="validateBooking()">
                    Đặt lịch
                </a>
            </div>
        </div>
    </div>
</div>
