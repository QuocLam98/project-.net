        function customFormatValue(value, number, char) {
            if (number == undefined || number == null) {
                number = 3;
            }
            if (char == undefined || char == null) {
                char = '.';
            }
            value = value != null ? value.toString() : "";
            var pattern = new RegExp(`\\B(?=(\\d{${number}})+(?!\\d))`, 'g');
