<section class="consultant_result sec_pad_happys">
    <div class="container">
        <div class="consultant_result_header">
            <h3 class="consultant_result_header_title m-0">Phiếu đánh giá tư vấn</h3>
            <h5 class="consultant_result_header_content m-0 d-none">Thông tin đang được cán bộ tư vấn cập nhật</h5>
        </div>
        <div class="line mt-3 mb-3">
        </div>
        <div class="consultant_result_body">
            <div class="consultant_card">
                <div class="consultant_card_title mb-3">
                    1. Thông tin cá nhân
                </div>
                <div class="consultant_card_body row mb-3">
                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Họ và tên</label>
                            <input type="text" class="input" id="full-name" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Ngày tháng năm sinh</label>
                            <input type="text" class="input" id="dob" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Giới tính (xác nhận khi sinh)</label>
                            <input type="text" class="input" id="gender" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Dân tộc - tôn giáo</label>
                            <input type="text" class="input" id="religiousNation" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Trình độ văn hóa</label>
                            <input type="text" class="input" id="culturalLevel" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Số điện thoại</label>
                            <input type="text" class="input" id="phone-number" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Chuyên gia thực hiện</label>
                            <input type="text" class="input" id="conselor-name" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>

                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Thời gian tham vấn</label>
                            <input type="text" class="input" id="consultingTime" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Hình thức tư vấn</label>
                            <input type="text" class="input" id="booking-type" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                    <div class="col-lg-12 col-sm-6 mb-3">
                        <div class="consultant_card_group">
                            <label class="title-input m-0">Địa chỉ</label>
                            <input type="text" class="input" id="address-detail" placeholder="Chưa có dữ liệu" disabled readonly>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="line mt-3 mb-3">
        </div>
